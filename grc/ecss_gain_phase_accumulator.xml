<?xml version="1.0"?>
<block>
  <name>Gain Phase Accumulator</name>
  <key>ecss_gain_phase_accumulator</key>
  <category>[ecss]</category>
  <import>import ecss</import>
  <make>ecss.gain_phase_accumulator($rst, $uplink, $downlink)</make>
  <callback>set_reset($rst)</callback>
  <callback>set_uplink($uplink)</callback>
  <callback>set_downlink($downlink)</callback>

	<param>
		<name>Reset</name>
		<key>rst</key>
		<value>0</value>
		<type>int</type>
	</param>
	<param>
		<name>uplink</name>
		<key>uplink</key>
		<value>221</value>
		<type>int</type>
	</param>
  	<param>
		<name>downlink</name>
		<key>downlink</key>
		<value>240</value>
		<type>int</type>
	</param>
	<sink>
		<name>in</name>
		<type>s64</type>
	</sink>
	<source>
		<name>out</name>
		<type>s64</type>
	</source>
</block>
