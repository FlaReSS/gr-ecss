<?xml version="1.0"?>
<block>
  <name>Variables loop Filter</name>
  <key>variables_loop_filter</key>
  <category>[ecss]</category>
  <import>from ecss import variables_loop_filter</import>

  <var_make>self.$(id) = $(id) = variables_loop_filter.coefficients($natural_freq_2, $damping_2, $index_m_3, $natural_freq_3, $damping_3, $samp_rate)</var_make>
  <var_value>variables_loop_filter.coefficients($natural_freq_2, $damping_2, $index_m_3, $natural_freq_3, $damping_3, $samp_rate)</var_value>
  <make></make>

  <param>
    <name>Natural frequency (2nd order)</name>
    <key>natural_freq_2</key>
    <type>real</type>
  </param>
  <param>
    <name>Damping (2n order)</name>
    <key>damping_2</key>
    <type>real</type>
  </param>
  <param>
    <name>Index m (3rd order)</name>
    <key>index_m_3</key>
    <type>int</type>
  </param>
  <param>
  <name>Natural frequency (3rd order)</name>
  <key>natural_freq_3</key>
  <type>real</type>
</param>
<param>
  <name>Damping (3rd order)</name>
  <key>damping_3</key>
  <type>real</type>
</param>
  <param>
    <name>Sampling rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
</block>
